#: usersd/user.py:121
msgid "Please complete the entire form."
msgstr "Si prega di completare tutti i campi."

#: usersd/user.py:128
#, python-format
msgid "The username '%s' is already taken."
msgstr "Il nome utente '%s' già esiste."

#: usersd/user.py:137
#, python-format
msgid "The username must not contain the following characters: %s"
msgstr "Il nome utente non deve contenere i seguenti caratteri: %s"

#: usersd/user.py:142
msgid "The passwords do not match."
msgstr "Le password non corrispondono."

#: usersd/user.py:147
#, python-format
msgid "The password should be of at least %s characters."
msgstr "La password deve essere di almeno %s caratteri."

#: usersd/user.py:154 usersd/user.py:165
msgid "Something went wrong while creating the new user."
msgstr "Qualcosa è andato storto durante la creazione del nuovo utente."

#: usersd/user.py:250
msgid "Current password is not correct."
msgstr "La password attuale non è corretta."

#: usersd/user.py:255
msgid "The new passwords do not match."
msgstr "Le nuove password non corrispondono."

#: usersd/user.py:260
#, python-format
msgid "The new password should be of at least %s characters."
msgstr "La nuova password deve essere di almeno %s caratteri."

